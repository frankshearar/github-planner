{
  "name": "GitHub Planning Helper",
  "version": "1.0",
  "manifest_version": 1,
  "description": "Add all numeric tags when viewing GitHub issues.",
  "content_scripts": [
    {
      "matches": [
          "https://github.com/*/*/issues*"
      ],
      "js": ["src/add-numeric-tags.js"]
    }
  ]
}
